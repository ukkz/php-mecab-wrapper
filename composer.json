{
    "name": "ukkz/php-mecab-wrapper",
    "description": "A wrapper class for mecab.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "mecab",
        "japanese",
        "wrapper",
        "language"
    ],
    "authors": [
        {
            "name": "uko",
            "email": "uko@aol.jp"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MeCab\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MeCabTest\\" : "tests/"
        }
    },
    "scripts": {
        "test": "php ./vendor/bin/phpunit"
    }
}
